<view class="w-picker"><view class="{{['mask',(showPicker)?'show':'']}}" catchtouchmove="__e" data-event-opts="{{[['tap',[['maskTap',['$event']]]],['touchmove',[['',['$event']]]]]}}" bindtap="__e"></view><view class="{{['w-picker-cnt',(showPicker)?'show':'']}}"><view class="w-picker-hd" catchtouchmove="__e" data-event-opts="{{[['touchmove',[['',['$event']]]]]}}"><block s-if="{{mode=='region'}}"><view data-event-opts="{{[['tap',[['pickerCancel',['$event']]]]]}}" class="w-picker-btn" bindtap="__e">取消</view></block><block s-if="{{mode=='date'||mode=='dateTime'||mode=='yearMonth'}}"><view class="w-picker-btn"><view class="dataType"><text data-event-opts="{{[['tap',[['setDateType',[1]]]]]}}" class="{{[(date_type==1)?'current':'']}}" bindtap="__e">公历</text><text data-event-opts="{{[['tap',[['setDateType',[2]]]]]}}" class="{{[(date_type==2)?'current':'']}}" bindtap="__e">农历</text></view></view></block><view data-event-opts="{{[['tap',[['pickerConfirm',['$event']]]]]}}" class="w-picker-btn" style="{{'color:'+(themeColor)+';'}}" bindtap="__e">确定</view></view><block s-if="{{mode=='date'||mode=='dateTime'||mode=='yearMonth'}}"><view class="w-picker-view"><picker-view class="vue-ref" indicator-style="height: 40px;" value="{{pickVal}}" data-ref="pic" data-event-opts="{{[['change',[['bindChange',['$event']]]]]}}" bindchange="__e"><picker-view-column><block s-for="{{data.years}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+"年"}}</view></block></picker-view-column><picker-view-column><block s-for="{{data.months}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+(f?'':'月')}}</view></block></picker-view-column><block s-if="{{mode!='yearMonth'}}"><picker-view-column><block s-for="{{data.days}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+(f?'':'日')}}</view></block></picker-view-column></block><block s-if="{{mode=='dateTime'}}"><picker-view-column><block s-for="{{data.hours}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+"时"}}</view></block></picker-view-column></block><block s-if="{{mode=='dateTime'}}"><picker-view-column><block s-for="{{data.minutes}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+"分"}}</view></block></picker-view-column></block></picker-view></view></block><block s-if="{{mode=='time'}}"><view class="w-picker-view"><picker-view indicator-style="height: 40px;" value="{{pickVal}}" data-event-opts="{{[['change',[['bindChange',['$event']]]]]}}" bindchange="__e"><picker-view-column><block s-for="{{data.hours}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+"时"}}</view></block></picker-view-column><picker-view-column><block s-for="{{data.minutes}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+"分"}}</view></block></picker-view-column><picker-view-column><block s-for="{{data.seconds}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item+"秒"}}</view></block></picker-view-column></picker-view></view></block><block s-if="{{mode=='region'}}"><view class="w-picker-view"><picker-view indicator-style="height: 40px;" value="{{pickVal}}" data-event-opts="{{[['change',[['bindChange',['$event']]]]]}}" bindchange="__e"><picker-view-column><block s-for="{{data.provinces}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item.label}}</view></block></picker-view-column><picker-view-column><block s-for="{{data.citys}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item.label}}</view></block></picker-view-column><picker-view-column><block s-for="{{data.areas}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item">{{item.label}}</view></block></picker-view-column></picker-view></view></block></view></view>